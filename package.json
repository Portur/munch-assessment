{
  "name": "template-node-api",
  "version": "0.0.1",
  "description": "A template api for node",
  "main": "server.js",
  "scripts": {
    "test": "jest --config jest.config.ts",
    "dev": "ts-node src/swagger.ts & npx ts-node src/server.ts",
    "build:dev": "rm -rf dist && tsc && cp -R node_modules dist/node_modules && cp environment/local_example.env dist/.env",
    "build:docker": "rm -rf dist && tsc && cp -R node_modules dist/node_modules && cp environment/docker.env dist/.env",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:dev:new": "prisma migrate dev --create-only"
  },
  "engines": {
    "npm": "10",
    "node": "20.15.1"
  },
  "author": "Joshua Toth",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^18.19.55",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^3.1.7",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^10.0.0"
  }
}
